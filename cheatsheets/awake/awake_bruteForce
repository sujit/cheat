# Match if the same activity takes place 300 times (within 5s)
util.db.bruteForceDetectionBy ["device", "activity.ip.destination"]
   300  /* No. of activities */
   5s   /* Time window */
   (
      allExceptionConditionsAreFalse && <custom_conditions>
   )
